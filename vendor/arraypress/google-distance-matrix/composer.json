{
  "name": "arraypress/google-distance-matrix",
  "version": "1.0.0",
  "description": "A PHP library for integrating with the Google Distance Matrix API in WordPress, providing travel distance and time calculations between multiple origins and destinations. Features WordPress transient caching and WP_Error support.",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/google-distance-matrix/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\Google\\DistanceMatrix\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/arraypress/google-distance-matrix/issues",
    "source": "https://github.com/arraypress/google-distance-matrix"
  },
  "keywords": [
    "distance-matrix",
    "travel-distance",
    "route-calculation",
    "shipping-distance",
    "delivery-time",
    "google-maps",
    "distance-calculation",
    "wordpress",
    "travel-time",
    "multi-origin",
    "shipping-zones",
    "delivery-routing",
    "ecommerce-shipping",
    "location-distance",
    "transit-time"
  ]
}